[Unit]
Description=MTProxy
After=network.target

[Service]
Type=simple
WorkingDirectory=/usr/local/src/MTProxy/objs/bin
ExecStart=/usr/local/src/MTProxy/objs/bin/mtproto-proxy -u nobody -p 8888 -H {{ mtproxy_port }} -S {{ connect_secret_decode }} --aes-pwd proxy-secret proxy-multi.conf -M 1
Restart=on-failure

[Install]
WantedBy=multi-user.target
